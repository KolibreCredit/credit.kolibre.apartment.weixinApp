/* pages/index/index.wxss */
page {
  background-color: #f4f4f4;
}

.topFilter1 {
  position: relative;
}

.topFilter2 {
  position: fixed;
  top: 0;
  z-index: 2;
}

.search {
  background: #ffffff;
  width: 100%;
  height: 44px;
  border-bottom: 1px solid #e5e5e5;
}

.search .item {
  height: 44px;
  line-height: 44px;
  float: left;
  font-size: 14px;
  color: #646464;
  text-align: center;
  position: relative;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.item .line {
  position: absolute;
  right: 0;
  width: 2px;
  height: 16px;
  top: 14px;
}

.item .jiantou {
  width: 14px;
  height: 14px;
  vertical-align: middle;
}

.item .sort {
  width: 24px;
  height: 24px;
  vertical-align: middle;
}

.item .model {
  width: 24px;
  height: 24px;
  vertical-align: middle;
}

.item .active {
  color: #fb9328;
}

.apartments {
  height: 50px;
  white-space: nowrap;
  margin-left: 2px;
  margin-right: 2px;
  z-index: 1
}

.apartments .item {
  background: #f5f5f5;
  border-radius: 3px;
  width: 90px;
  height: 30px;
  padding: 0 5px;
  line-height: 30px;
  margin-right: 10px;
  margin-top: 10px;
  font-size: 14px;
  color: #646464;
  text-align: center;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block
}

.apartments .item image {
  display: none;
}

.apartments .active {
  background: #FFF3E7;
  color: #fb9329;
}

.apartments .active image {
  display: block;
  width: 20px;
  height: 19px;
  position: absolute;
  right: 0;
  top: 0;
}

.filter {
  position: absolute;
  top: 44px;
  left: 0;
  right: 0;
  height: auto;
  padding-top: 20px;
  z-index: 3;
  background-color: #ffffff;
}

.filter .item {
  text-align: center;
  width: auto;
  font-size: 14px;
  color: #646464;
  height: 40px;
  line-height: 44px;
}

.filter .active {
  color: #fb9328;
}

.viewContent1 {
  margin-top: 0px;
}

.viewContent2 {
  margin-top: 100px;
}

.list {
  background-color: white;
}

.list .item {
  border-top: 1px solid #f1f1f1;
  position: relative;
  text-align: left;
  height: 130px;
  padding-left: 160px;
  padding-right: 20px;
  padding-top: 15px;
  width: 100%;
}

.item .picture {
  position: absolute;
  width: 120px;
  height: 90px;
  top: 20px;
  left: 20px;
}

.item .apartmentName {
  font-size: 16px;
  color: #646464;
  max-height: 44px;
  line-height: 22px;
  /*多行*/
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.item .retailPrice {
  font-size: 16px;
  color: #fb9328;
  height: 22px;
  line-height: 22px;
}

.item .addr {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px;
  color: #7b7b7b;
  height: 22px;
  line-height: 22px;
  position: relative;
  padding-left: 15px;
}

.item .addr image {
  position: absolute;
  width: 9px;
  height: 12px;
  top: 6px;
  left: 0;
}

.item .roomSourceFeature {
  height: 20px;
  line-height: 20px;
  margin-top: 5px;
}

.item .roomSourceFeature span {
  display: inline-block;
  padding: 0 5px;
  font-size: 12px;
  color: #7d7d7d;
  border: 1px solid #cccccc;
  margin-right: 10px;
  border-radius: 1px;
  max-width: 62px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}

.listColumn {
  background-color: white;
}

.listColumn .itemColumn {
  padding-top: 10px;
}

.itemColumn .picture {
  width: 100%;
  height: 240px;
}

.itemColumn .retailPrice {
  font-size: 16px;
  color: #ffffff;
  height: 40px;
  line-height: 40px;
  position: absolute;
  left: 20px;
}

.itemColumn .roomSourceFeature {
  position: absolute;
  top: 8px;
  right: 20px;
}

.itemColumn .roomSourceFeature span {
  display: inline-block;
  padding: 0 5px;
  font-size: 12px;
  color: #ffffff;
  max-width: 62px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  border-radius: 2px;
  margin-left: 10px;
}

.itemColumn .roomSourceFeature span:nth-child(0) {
  background: #ff8c14;
}

.itemColumn .roomSourceFeature span:nth-child(1) {
  background: #ffc401;
}

.itemColumn .roomSourceFeature span:nth-child(2) {
  background: #a2ce6c;
}

.itemColumn .apartmentName {
  font-size: 16px;
  color: #646464;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 36px;
  line-height: 36px;
  background-color: #ffffff;
  padding: 0 20px;
  text-align: left;
}

.itemColumn .addr {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px;
  color: #7b7b7b;
  position: relative;
  background-color: #ffffff;
  text-align: left;
  padding-bottom: 8px;
  padding-left: 32px;
}

.itemColumn .addr image {
  width: 9px;
  height: 12px;
  position: absolute;
  left: 20px;
  top: 4px;
}

.errData {
  display: flex;
  justify-content: center;
  height: 100vh;
  background-color: white;
  align-items: center;
}